/*background-color: white;*/
/*}*/

/* Useful deviations from regular style on elements */
/* List classes without markers */
ul.visualNoMarker,
ol.visualNoMarker {
    list-style: none none;
    margin: 0.5em 0 0 0;
    line-height: 1em;
}


/* Common file types icon */
.link-plain {
    background-image: none;
    padding: 0;
}

.link-parent {
    display: block;
    background: transparent url(&dtml-xurl;/arrowUp.gif) 4px 5px no-repeat;
    padding: 1px 0 10px 16px;
    font-size: &dtml-fontSmallSize;;
    text-decoration: none;
}

.link-external {
    background: transparent url(&dtml-xurl;/link_icon.gif) 3px 1px no-repeat;
    padding: 1px 0 1px 19px;
}

/* Links with differently colored link underlines - only for content */
#content span.link-external  {
    background: none;
    padding: 0;
}

.link-feed, .link-rss, .link-rdf, .link-xml {
    background: transparent url(&dtml-xurl;/rss.gif) 0 1px no-repeat;
    padding: 1px 0 1px 18px;
}

.link-comment {
    background: transparent url(&dtml-xurl;/discussionitem_icon.gif) center left no-repeat;
    padding: 1px 0 1px 16px !important; /* value:  */
}

.link-anchor {
    color: &dtml-discreetColor;;
    text-decoration: none;
    font-weight: normal;
}

.link-pdf {
    background: transparent url(&dtml-xurl;/pdf.png) 0 50% no-repeat;
    padding: 2px 0 2px 16px;
}
.link-xlb, .link-xls, .link-xlt, .link-csv, .link-tsv {
    background: transparent url(&dtml-xurl;/xls.png) 0 50% no-repeat;
    padding: 2px 0 2px 20px;}
.link-json {
    background: transparent url(&dtml-xurl;/script_code.png) 0 50% no-repeat;
    padding: 2px 0 2px 20px;}
.link-exhibit {
    background: transparent url(&dtml-xurl;/script_code_red.png) 0 50% no-repeat;
    padding: 2px 0 2px 20px;}
.link-table {
    background: transparent url(&dtml-xurl;/table.png) 0 50% no-repeat;
    padding: 2px 0 2px 20px;}

/* @end Common file types icon */

/* TinyMCE image alignment classes */
.image-left {
    float: left;
    clear: both;
    margin:  2em 1em 0.5em 0;
    border: 0;
}
.image-inline {
    float: none;
}
.image-right {
    float: right;
    clear: both;
    margin: 0.5em;
    border: 0;
}
/* @end TinyMCE image alignment classes */

/* tinymce class to hide or show content when printing */
.showOnPrint {
    display: none;
}
.mceContentBody .showOnPrint {
    display: block;
}
/* @end tinymce class to hide or show content when printing */

/* custom css fixes and overrides give links or detailed fix information */

/* eeareadabilitychecker stats */
#stats td {
    padding-right: 1em;
}

/* Override jquery.ui style */
.ui-widget-content a {
    color: &dtml-linkColor;;
}

.frontpage .progressbar-viewlet,
.template-datamaps_view .progressbar-viewlet
{
    display: none;
}

/* custom css fix for hiding warning message for anonymous users when  visualization is not published */
.userrole-anonymous .metadata ~ .warningMessage {
    display: none;
}

/* do not show copyright info on soer briefings */
.fiche-copyright {
    display: none;
}

/* #81310 table should be visible overriding bootstrap invisible class */
table.invisible {
    visibility: visible;
}

.template-collection-pdf-body h4.figureHeading {
    margin: 0 0 1.5em 0;
}

.portaltype-assessment #parent-fieldname-description {
    display: none;
}

/* eea.googlecharts chart.js uses this */
div#original_chart_div {
    overflow: hidden;
}
/* #50513 hide global filter for document register
* so that users cannot remove it.
* https://www.eea.europa.eu/about-us/documents/administrativedocuments facet entry
*/
#c4_option_c4_public-administrative-document {
    display: none !important;
}

/* #70675 show reply to comment button */
.reply-to-comment-button {
    display: inline !important;
}

.figures-download-links > li {
    display: list-item;
}

/* #5454; we have required spans where we don't need the background image */
.no-bg:after {
    background-image: none !important;
    content: "" !important;
}

.qrbox {
    display: none;
}

/* 5455 fix publicationMessage link color
* older version portlet https://www.eea.europa.eu//publications/term-report-2016
*/
#content dl.publicationMessage a {
    border: 0;
    color: &dtml-linkColor;;
}
/* @end custom css fixes and overrides */


