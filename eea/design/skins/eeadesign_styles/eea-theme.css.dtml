/* no horisontal lines allowed, only used as spacer, separation is done with headings or bg colors */
hr {
    border: none 0;
    border-top: 1px solid white;/*invisible border*/
    height: 0px;
    margin: 0.5em 0 1em 0;
}

.eea-dates {
    margin: 1em 1em 1em 0;
}

/* portal-header area */
/* siteactions cross_site_top */
/* use font awesome unicode icons */
/* daviz -edit sets eea-icon color to #000 */
#portal-siteactions #siteaction-chooselang .eea-icon {
    color: #FFF;
}

#siteaction-contactus {
    font-weight: bold;
}

/* @end siteactions cross_site_top */
#cross-site-top .panel .portlet-subscribtion {
    margin: 0;
}
#cross-site-top .panel .portlet-subscribtion .portletFooter {
    margin-bottom: 0;
}
#cross-site-top .panel .portlet,
#cross-site-top .panel .portletHeader {
    background: transparent;
}
#tip-siteaction-chooselang,
#tip-siteaction-chooselang-menu {
    width: 145px;
}
#tip-siteaction-chooselang li,
#tip-siteaction-chooselang-menu li {
    display: list-item;
    margin: 0;
    text-align: left;
}
#tip-siteaction-subscriptions,
#tip-siteaction-subscriptions-menu {
    width: 240px;
}
#tip-siteaction-contactus,
#tip-siteaction-contactus-menu {
    font-size: 125%;
    width: 420px;
}
#tip-externalsites-networks, #tip-externalsites-etcssites {
    width: 400px;
    left: 0 !important;
}
/* Networks panel EU partners links */
.moreLink {
    border: 0 !important;
    display: inline-block;
    padding-left: 20px;
    height: 18px;
    font-size: 11px;
    line-height: 1.6;
    position: relative;
}

.moreLink:before {
    content: "\f138";
    font-family: FontAwesome;
    font-size: 16px;
    color: gray;
    position: absolute;
    margin-left: -18px;
    top: -3px;
}
/* End links */

/* Site-wide action menu - font size, contact, index, sitemap etc */
#portal-siteactions > #siteaction-networks, #portal-siteactions > #siteaction-search {
    display: none;
}
.mini-header .container {
    display: flex;
    justify-content: space-between;
}

/* subscription cross_site_top panel content icons */
.portlet-subscription .eea-icon-lg {
    line-height: 0.75em;
    width: 20px;
}

.eea-icon-facebook-square {
    color: #596EA0;
}
.eea-icon-twitter-square {
    color: #1da1f2;
}
.eea-icon-rss-square {
    color: #ED8628;
}
.eea-icon-youtube-square {
    color: #DB2B28;
}
/* @end subscription cross_site_top panel content icons */

.mini-header #portal-header {
    height: 100%;
}
#portal-header.eea-miniheader-element {
    display: block;
    margin: 2em auto;
}

/* Searchbox style and positioning */
#portal-searchbox .gsc-clear-button {
    display: none;
}

#portal-searchbox .gsc-control-cse {
    background-color: transparent;
    padding: 0;
    border: 0;
}

.panel #portal-searchbox {
    float: none;
    clear: both;
    margin: 1em;
    width: 400px;
    overflow: hidden;
}

#portal-searchbox a {
    color: &dtml-linkColor;;
    border: none;
    font-size: 85%;
}

#portal-searchbox .gsc-input {
    padding: 0 0.5em;
    background: url("//www.google.com/cse/static/images/1x/googlelogo_lightgrey_46x16dp.png") 5px center no-repeat rgb(255, 255, 255) !important;
}
#portal-searchbox .gsc-input:focus {
    background: #FFF none !important;
}
#portal-searchbox .gsc-input {
    padding: 0;
}

#portal-searchbox .searchButton:focus {
    background-color: &dtml-corporateOrange;;
}

#portal-searchbox .gsc-cursor-current-page {
    color: #FFF;
}

#portal-searchbox .gsc-results-wrapper-overlay {
    height: 96%;
    top: 2%;
    width: 90%;
    left: 5%;

}
#search_glossary_link, #search_advanced_link {
    font-size: 11px;
}

#search_glossary_link:hover, #search_advanced_link:hover {
    text-decoration: none;
}
/* @end Searchbox style and positioning */
/* @end portal-header area */

/* breadcrumbs found below global-nav*/
.breadcrumbSeparator {
    font-size: 120%;
}
/* @end breadcrumbs */

/* #23500 allow the control panel widget to be placed outside of their context
    by having overflow visible for screen with size larger than 1024px
*/
#region-content {
    position: relative;
}
#content-core {
    clear:both;
}

/* portal-column styles */

.flowplayer a {
    display: inline;
}

.visualPadding {
    padding: 0 1em 1em 1em;
}
#portal-column-two .visualPadding {
    padding: 0 1em 1em 0;
}

/* Language viewlet */
.documentActions li, .languageCodes li {
    display: inline-block !important;
    list-style: none none;
    margin: 0;
    padding: 0;
}
.languageCodes {
    display: block;
}
#languageCodes {
    max-width: 80%;
}
#article-language .eea-icon {
    line-height: 0.75em;
    color: #888;
}
.width-full #article-language {
    min-width: 180px;
}
/* @end Language viewlet */

/* download.pdf download.epub export actions */
/* download.pdf extra clicking prevention styles */
a.downloading {
    background: transparent url(&dtml-xurl;/icon-downloading.gif) no-repeat center center;
}

a.downloading img {
    visibility: hidden;
}

.downloading-lock::before {
    content: '';
    display: block;
    width: 100%;
    height: 100%;
    position: absolute;
    right: 0;
    z-index: 9999;
}
/* @end download.pdf extra clicking prevention styles */

#region-content .documentExportActions ul, .documentExportActions ul {
    list-style: none;
    padding: 0;
    margin: 1em -0.15em 0 0;
}

#region-content ul,
#chooselang {
    list-style: square inside url(//www.eea.europa.eu/bullet.gif);
}

#region-content .documentExportActions li, .documentExportActions li {
    display: inline;
}

.eea-footer-right .link-pdf,
.eea-footer-right .link-epub,
#region-content .documentExportActions .link-pdf,
#region-content .documentExportActions .link-epub,
.documentExportActions .link-pdf,
.documentExportActions .link-epub {
    background: none;
    padding: 0;
    margin: 0;
}

#content .eea-footer-right a.download-pdf,
#content .eea-footer-right a.download-epub {
    border-bottom: none !important;
}

#content .documentExportActions a {
    border-bottom: none !important;
    text-align: center;
}

.documentExportActions .export-action-title {
    display: block;
}

.documentExportActions .eea-icon::before {
    margin: 0 0.15em;
}

/* 69065 clear content that follows .documentExportActions */
.eea-available-version,
#plone-document-byline + .portalMessage,
#plone-document-byline + div {
    clear: both;
}

#content .documentByLine {
    max-width: 80%;
    text-align: left;
}

.portaltype-fiche .link-presentation {
    display: none;
}

.eea-epub-download,
.eea-pdf-download {
    font-size: 150%;
}

div.eea-pdf-overlay {
    z-index: 1031;
}
/* @end download.pdf download.epub export actions */

/* documentByLine */
.documentByLine {
    font-size: &dtml-fontSmallSize;;
    font-weight: normal;
    color: &dtml-discreetColor;;
    margin: 0 0 0.5em 0;
}
.documentByLine .portalType, .listHead {
    font-weight: bold;
}
.documentByLineAuthor:nth-child(2) span:after,
.documentByLineAuthor a:after, .documentByLineContributor a:after {
    content: ',';
}
.documentByLineAuthor:nth-child(2) a:after,
.documentByLineAuthor:last-child a:after, .documentByLineContributor:last-child a:after {
    content: '';
}

/* @end documentByLine */

#viewlet-below-content {
    clear: both;
}
/* relatedItems */
#relatedItems  {
    margin: 0 0 0 0;
    clear: both;
    overflow: hidden;
}
/* #19315 fix floating issues */
#relatedItems  h3 {
    clear: both;
}
/* @end relatedItems */

/* #10182 pagination style */
.pagination .current:before {
    content: "[";
}
.pagination .current:after {
    content: "]";
}
.page:before,
.page:after {
    display: table;
    content: " ";
}
.page:after {
    clear: both;
}
.spinner {
    text-align: center;
}
/* @end #10182 pagination style */

.contentActions {
    clear: both;
}
/*.width-full .content-header {
    background-color: white;
}*/

/* 70523 set callout to a base of 12px in order to normalize googlechart notes size  */
#content .callout {
    font-size: 12px;
}

/*.template-facetednavigation_view #socialmedia-viewlet,*/
