        float: left;
        margin: 0!important;
        padding: 0!important;
    }
    /* Clear floated elements */
    .tabbedmenu ul:before,
    .tabbedmenu ul:after {
        display: table;
        content: " ";
    }
    .tabbedmenu ul:after {
        clear: both;
    }
    .tabbedmenu ul > li a {
        display: block;
        color: #fff!important;
        text-shadow: 1px 1px 1px #000000;
        text-decoration: none;
        white-space: nowrap;
        padding: 8px 8px;
    }
    .tabbedmenu.two-rows ul > li a {
        height: 50px;
        white-space: normal;
        max-width: 150px;
    }
    .tabbedmenu ul > li a:hover {
        color: &dtml-corporateBlueTwo;!important;
        text-decoration: none;
        text-shadow: 1px 1px 1px #FFFFFF;
        background-color: &dtml-greyBackgroundColor;;
    }
    .tabbedmenu ul > li a.current,
    .tabbedmenu ul > li a.current:hover {
        cursor: default;
        color: &dtml-corporateBlueTwo;!important;
        text-decoration: none;
        text-shadow: 1px 1px 1px #FFFFFF;
        background-color: &dtml-greyBackgroundColor;;
    }
    .border-tabbedmenu,
    .tabbedmenu-panel {
        border: 1px solid #DDD;
        border-top: 0;
        padding: 10px;
    }
}

.qrbox {
    display: none;
}

/* download.pdf extra clicking prevention styles */
a.downloading {
    background: transparent url(&dtml-portal_url;/icon-downloading.gif) no-repeat center center;
}

a.downloading img {
    visibility: hidden;
}

.downloading-lock::before {
    content: '';
    display: block;
    width: 100%;
    height: 100%;
    position: absolute;
    right: 0;
    z-index: 9999;
}

/* eeareadabilitychecker stats */
#stats td {
    padding-right: 1em;
}

/* eea.googlecharts chart.js uses this */
div#original_chart_div {
