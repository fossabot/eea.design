}

/* 71710 faceted title widget fixes */
.faceted-widget fieldset {
    margin: 1em;
}
.faceted-widget .ui-widget-header {
    margin: -1em -1em 1em -1em;
}

.faceted-portlet-widget .ui-widget-header {
    margin: -1em -1em 0;
}

#faceted-form .widget-fieldset legend {
